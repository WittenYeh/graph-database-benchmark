{
  "neo4j": {
    "docker_image": "bench-neo4j",
    "dockerfile_path": "./docker/neo4j/Dockerfile",
    "container_name": "neo4j-benchmark",
    "api_port": 50080,
    "runtime": "java",
    "config": {
      "heap_size": "4G",
      "page_cache": "2G"
    }
  },
  "janusgraph": {
    "docker_image": "bench-janusgraph",
    "dockerfile_path": "./docker/janusgraph/Dockerfile",
    "container_name": "janusgraph-benchmark",
    "api_port": 50081,
    "runtime": "java",
    "config": {
      "heap_size": "4G",
      "storage_backend": "berkeleyje"
    }
  },
  "arangodb": {
    "docker_image": "bench-arangodb",
    "dockerfile_path": "./docker/arangodb/Dockerfile",
    "container_name": "arangodb-benchmark",
    "api_port": 50082,
    "runtime": "cpp",
    "config": {
      "server_threads": 8
    }
  },
  "orientdb": {
    "docker_image": "bench-orientdb",
    "dockerfile_path": "./docker/orientdb/Dockerfile",
    "container_name": "orientdb-benchmark",
    "api_port": 50083,
    "runtime": "java",
    "config": {
      "heap_size": "4G"
    }
  }
}
